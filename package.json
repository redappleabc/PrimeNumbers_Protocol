{
  "name": "PrimeProtocol",
  "version": "2.3.0",
  "description": "Prime Omni-Chain Money Market",
  "packageManager": "yarn@3.6.3",
  "scripts": {
    "prepare": "node ./.setup.js && hardhat typechain",
    "format": "prettier --check \"**/*.{ts,js,sol}\"",
    "format:fix": "prettier --write \"**/*.{ts,js,sol}\"",
    "compile": "hardhat compile",
    "test": "npx hardhat test",
    "node": "cross-env HH_LOG=true npx hardhat node --no-deploy",
    "gas": "cross-env REPORT_GAS=true hardhat test",
    "coverage": "cross-env HARDHAT_DEPLOY_FIXTURE=false hardhat coverage",
    "dev:node": "cross-env MINING_INTERVAL=\"3000,5000\" hardhat node --hostname 0.0.0.0",
    "dev": "cross-env MINING_INTERVAL=\"3000,5000\" hardhat node --hostname 0.0.0.0 --watch",
    "local:dev": "hardhat --network localhost deploy --watch",
    "execute": "node ./_scripts.js run",
    "deploy": "node ./_scripts.js deploy",
    "deploy:sepolia": "npx hardhat --network sepolia deploy",
    "verify": "node ./_scripts.js verify",
    "export": "node ./_scripts.js export",
    "hardhat": "hardhat",
    "fork:execute": "node ./_scripts.js fork:run",
    "fork:deploy": "node ./_scripts.js fork:deploy",
    "fork:dev": "node ./_scripts.js fork:dev",
    "fork:node": "node ./_scripts.js fork:node",
    "fork:test": "node ./_scripts.js fork:test",
    "tenderly:push": "node ./_scripts.js tenderly:push",
    "size": "npx hardhat size-contracts",
    "test-ci": "IS_CI=true npx hardhat test",
    "solhint": "npx solhint 'contracts/**/*.sol'",
    "mine": "npx hardhat run scripts/setIntervalMining.ts --network localhost",
    "skip": "npx hardhat run scripts/skip.ts --network localhost"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@nomicfoundation/hardhat-chai-matchers": "1",
    "@nomicfoundation/hardhat-ignition": "^0.15.5",
    "@nomicfoundation/hardhat-ignition-ethers": "^0.15.5",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.11",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "@nomicfoundation/ignition-core": "^0.15.5",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-etherscan": "^3.1.8",
    "@openzeppelin/contracts": "^4.9.6",
    "@typechain/ethers-v5": "^11.1.2",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.4",
    "@types/lodash": "^4.14.191",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.13.0",
    "chai": "4",
    "chai-ethers": "^0.0.1",
    "cross-env": "^7.0.2",
    "dotenv": "^16.0.3",
    "ethers": "5.7.2",
    "fs-extra": "^11.1.0",
    "hardhat": "^2.22.6",
    "hardhat-deploy": "^0.12.4",
    "hardhat-deploy-ethers": "0.4.0-next.1",
    "hardhat-deploy-tenderly": "^0.2.0",
    "hardhat-gas-reporter": "^2.2.0",
    "mocha": "^10.2.0",
    "prettier": "^2.8.4",
    "prettier-plugin-solidity": "^1.1.2",
    "solidity-coverage": "^0.8.12",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.9.1",
    "typechain": "^8.3.2",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@balancer-labs/v2-pool-utils": "^4.1.1",
    "@gnosis.pm/safe-core-sdk": "^3.2.0",
    "@gnosis.pm/safe-ethers-lib": "^1.7.0",
    "@gnosis.pm/safe-service-client": "^1.4.0",
    "@layerzerolabs/lz-evm-oapp-v2": "^2.3.42",
    "@layerzerolabs/lz-sdk": "^0.0.12",
    "@layerzerolabs/solidity-examples": "^0.0.8",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/contracts-upgradeable": "^4.9.3",
    "@openzeppelin/hardhat-defender": "^1.8.2",
    "@openzeppelin/hardhat-upgrades": "1.28.0",
    "@openzeppelin/upgrades-core": "^1.34.1",
    "@stargatefinance/stg-evm-v2": "^1.0.16",
    "@tenderly/hardhat-tenderly": "^1.7.7",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v3-core": "1.0.2-solc-0.8-simulate",
    "@uniswap/v3-periphery": "github:Uniswap/v3-periphery.git#0.8",
    "cli-ux": "^6.0.9",
    "defender-admin-client": "^1.39.0",
    "defender-relay-client": "^1.39.0",
    "ethereum-waffle": "^4.0.10",
    "git": "^0.1.5",
    "glob": "^9.2.1",
    "hardhat-contract-sizer": "^2.8.0",
    "hardhat-ignore-warnings": "^0.2.9",
    "hardhat-storage-layout-changes": "^0.1.2",
    "solc": "^0.8.19",
    "solhint": "^3.4.1",
    "solidity": "^0.0.1",
    "tiny-invariant": "^1.3.1",
    "web3": "^1.8.2",
    "yarn": "^1.22.19"
  }
}
